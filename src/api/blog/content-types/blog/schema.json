{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Category": {
      "type": "string"
    },
    "Excerpt": {
      "type": "richtext"
    },
    "BlogThumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "BlogContent": {
      "type": "richtext"
    },
    "Tags": {
      "displayName": "Tags",
      "type": "component",
      "repeatable": true,
      "component": "tags.tags"
    },
    "PostedBy": {
      "type": "string"
    },
    "PostedDate": {
      "type": "date"
    },
    "AuthorImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "PostURL": {
      "type": "uid",
      "targetField": "Title"
    },
    "seo": {
      "displayName": "SEO",
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    }
  }
}
